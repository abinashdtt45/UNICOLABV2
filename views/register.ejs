        <section class="page-section cta">
            <div class="container">
                <div class="row">
                    <div class="col-xl-9 mx-auto">
                        <div class="cta-inner text-center rounded">
                            <h2 class="section-heading mb-5">
                                <span class="section-heading-lower">
                                    Register
                                </span>
                            </h2>
                            <div class="row">
                                <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
                      
                                            <form class="form-signin" action="/register" method="post">
                                              <div class="form-label-group">
                                                    <input class="form-control" id="name" placeholder="" required="" type="name" name="name">
                                                        <label for="name">
                                                            Name
                                                        </label>
                                                    </input>
                                                </div>
                                                <div class="form-label-group">
                                                    <input autofocus="" class="form-control" id="inputEmail" placeholder="" required="" type="email" name="email">
                                                        <label for="inputEmail">
                                                            Email address
                                                        </label>
                                                    </input>
                                                </div>
                                                <div class="form-label-group">
                                                    <input class="form-control" id="inputPassword" placeholder="" required="" type="password" name="password">
                                                        <label for="inputPassword">
                                                            Password
                                                        </label>
                                                    </input>
                                                </div>
                                                <div class="form-label-group">
                                                    <input class="form-control" id="confirminputPassword" placeholder="" required="" type="password" name="password2">
                                                        <label for="confirminputPassword">
                                                            Confirm Password
                                                        </label>
                                                    </input>
                                                </div>
                                                <button class="btn btn-lg btn-primary btn-block text-uppercase" type="submit">
                                                    Create Account
                                                </button>
                                                <hr>
                                                Already have a account? <a href="/login"> Log In</a>
                                                
                                            </form>
                                        
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
